<h1>New Homeowner</h1>


<%= form_tag(homeowners_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

    <div>
    <%= label_tag :phone %><br />
    <%= text_field_tag :phone %>
  </div>

    <div>
    <%= label_tag :address %><br />
    <%= text_field_tag :address %>
  </div>

  
  <div>
    <%= submit_tag "Create Homeowner" %>
  </div>
<% end %>
