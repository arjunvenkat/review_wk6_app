<h1>New Quote</h1>


<%= form_tag(quotes_url, method: 'post') do %>


  <div>
    <%= label_tag :job_id %><br />
    <%= number_field_tag :job_id %>
  </div>

    <div>
    <%= label_tag :contractor_id %><br />
    <%= number_field_tag :contractor_id %>
  </div>

    <div>
    <%= label_tag :price %><br />
    <%= text_field_tag :price %>
  </div>

  
  <div>
    <%= submit_tag "Create Quote" %>
  </div>
<% end %>
